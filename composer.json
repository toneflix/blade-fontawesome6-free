{
    "name": "toneflix-code/blade-fontawesome6-free",
    "type": "library",
    "description": "A package to easily make use of Font Awesome 6 in your Laravel Blade views.",
    "license": "MIT",
    "authors": [ 
        {
            "name": "Toneflix Code",
            "email": "toneflix.code@gmail.com",
            "homepage": "https://code.toneflix.com.ng",
            "role": "Owner"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "blade-ui-kit/blade-icons": "^1.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "ToneflixCode\\FontAwesome6\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ToneflixCode\\FontAwesome6\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "ToneflixCode\\FontAwesome6\\BladeFontAwesome6ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
